{
  "name": "begin-app",
  "version": "0.0.0",
  "description": "Address Lookup App",
  "arc": {
    "app": "crud-app",
    "http": [
      {
        "/addresses": {
          "method": "post",
          "src": "/addresses/create"
        }
      },
      {
        "/addresses": {
          "method": "get",
          "src": "/addresses/read"
        }
      },
      {
        "/addresses/delete": {
          "method": "post",
          "src": "/addresses/destroy"
        }
      },
      {
        "/partialAddresses/delete": {
          "method": "post",
          "src": "/partialAddresses/destroy"
        }
      },
      {
        "/addresses/:id": {
          "method": "post",
          "src": "/addresses/update"
        }
      },
      {
        "/search/:query": {
          "method": "get",
          "src": "/search/execute"
        }
      }
    ],
    "addresses": [
      {
        "data": {
          "scopeID": "*String",
          "dataID": "**String",
          "ttl": "TTL"
        }
      }
    ],
    "partialAddresses": [
      {
        "data": {
          "scopeID": "*String",
          "dataID": "**String",
          "ttl": "TTL"
        }
      }
    ]
  },
  "scripts": {
    "start": "npx sandbox"
  },
  "dependencies": {
    "@architect/functions": "^3.13.9",
    "@architect/sandbox": "^3.4.0",
    "@begin/data": "^2.0.1",
    "tiny-json-http": "^7.3.0"
  }
}
